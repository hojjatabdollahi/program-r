    <!-- This is the service method -->
    <category>
        <pattern>GET BBC NEWS</pattern>
        <template>
            <sraix service="NEWS">SOURCE bbc-news</sraix>
            <oob><robot></robot></oob>
        </template>
    </category>

    <category>
        <pattern>GET US NEWS</pattern>
        <template>
            <sraix service="NEWS">COUNTRY us</sraix>
            <oob><robot></robot></oob>
        </template>
    </category>

    <category>
        <pattern># <set>YES</set> ^</pattern>
        <that>SHOULD I READ THE FULL ARTICLE?</that>
        <template>
            <sraix service="NEWS">ARTICLE us</sraix>
            <oob><robot></robot></oob>
        </template>
    </category>

    <category>
        <pattern>^ NO ^</pattern>
        <that>SHOULD I READ THE FULL ARTICLE?</that>
        <template>
            Ok should we go on to the next article?
            <oob><robot></robot></oob>
        </template>
    </category>

    <category>
        <pattern># <set>YES</set> ^</pattern>
        <that>OK SHOULD WE GO ON TO THE NEXT ARTICLE?</that>
        <template>
            <sraix service="NEWS">NEXT us</sraix>
            <oob><robot></robot></oob>
        </template>
    </category>

    <category>
        <pattern># <set>NO</set> ^</pattern>
        <that>OK SHOULD WE GO ON TO THE NEXT ARTICLE?</that>
        <template>
            If you want me to go back to the last article say, back.
            Other wise we can talk about something else.
            <oob><robot></robot></oob>
        </template>
    </category>

    <category>
        <pattern>BACK</pattern>
        <that>OTHER WISE WE CAN TALK ABOUT SOMETHING ELSE?</that>
        <template>
            <sraix service="NEWS">PREVIOUS us</sraix>
            <oob><robot></robot></oob>
        </template>
    </category>


</aiml>