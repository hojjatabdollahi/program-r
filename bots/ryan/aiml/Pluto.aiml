    <category>
        <pattern>_</pattern>
        <that>HOW DO YOU FEEL ABOUT PLUTO NO LONGER BEING A PLANET?</that>
        <template>
            <think><set name="data"> <star /> </set></think>
            <condition name="getsentiment">
